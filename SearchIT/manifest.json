{
  "name": "SearchIT",
  "description": "Searches your query where you'd like.",
  "version": "1.0",
  "manifest_version": 2,

  "browser_action": {
    "default_title": "Select a website.",
    "default_icon": {
      "19": "icon2.png",
      "38": "icon.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": ["tabs", "activeTab", "http://*/*", "https://*/*"]
}
